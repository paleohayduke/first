#simple comparison program
varA = input("Enter a variable for A: ")
varB = input("Enter a variable for B: ")

if type(varA) == str or type(varB) == str:    #determines whether variables are strings first
    print ("string involved")
elif varA > varB:
    print ("bigger")
elif varA == varB:
    print ("equal")
elif varA < varB:
    print ("smaller")
else:
    print ("string involved")
